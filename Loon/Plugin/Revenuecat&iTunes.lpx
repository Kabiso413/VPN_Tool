http-response ^https?:\/\/buy\.itunes\.apple\.com\/verifyReceipt$ script-path=https://ddgksf2013.top/scripts/buyitunes.vip.js, requires-body=true, timeout=60, tag=iTunes-ddgksf2013, enable={iTunesScriptEnable-ddgksf2013}

[MITM]
hostname = api.lianjiu.fun, api.revenuecat.com, api.rc-backup.com, buy.itunes.apple.com
